<%= form_for(@physical) do |f| %>
  <% if @physical.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@physical.errors.count, "error") %> prohibited this physical from being saved:</h2>

      <ul>
      <% @physical.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label 'Tape ID' %><br>
    <%= f.text_field :tape %>
  </div>
  <div class="field">
    <%= f.label :new_box %><br>
    <%= f.text_field :new_box %>
  </div>
  <div class="field">
    <%= f.label :old_box %><br>
    <%= f.text_field :old_box %>
  </div>
  <div class="field">
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :label_title %><br>
    <%= f.text_field :label_title %>
  </div>
  <div class="field">
    <%= f.label :label_info %><br>
    <%= f.text_area :label_info %>
  </div>
  <div class="field">
    <%= f.label :date_label %><br>
    <%= f.date_select :date_label %>
  </div>
  <div class="field">
    <%= f.label :date_actual %><br>
    <%= f.date_select :date_actual %>
  </div>
  <div class="field">
    <%= f.label :owner_id %><br>
    <%= f.collection_select :owner_id, @owners, :id, :owner_name %>
  </div>
  <div class="field">
    <%= f.label :master_status %><br>
    <%= f.text_field :master_status %>
  </div>
  <div class="field">
    <%= f.label :tape_format_id %><br>
    <%= f.collection_select :tape_format_id, @tape_formats, :id, :tape_format %>
  </div>
  <div class="field">
    <%= f.label :brand_id %><br>
    <%= f.collection_select :brand_id, @brands, :id, :brand %>
  </div>
  <div class="field">
    <%= f.label :condition_id %><br>
    <%= f.collection_select :condition_id, @conditions, :id, :condition %>
  </div>
  <div class="field">
    <%= f.label :notes %><br>
    <%= f.text_area :notes %>
  </div>
  <div class="field">
    <%= f.label :location %><br>
    <%= f.text_field :location %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
