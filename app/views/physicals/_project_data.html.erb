<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Projects</h3>
  </div>
  <div class="panel-body">
    <% @physical.projects.each do |project| %>

      <span class="pull-right"><em><%= @physical.try(:date_production).try(:stamp, 'July 1, 2015') %></em></span>
      <h3 class="green-text no-top-margin"><%= project.title %></h3>
      <p><%= project.description %></p>

    <% end %>
  </div>
</div>