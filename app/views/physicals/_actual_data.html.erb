<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Actual Data</h3>
  </div>
  <div class="panel-body">
    <span class="pull-right text-muted"><em><%= @physical.try(:date_actual).try(:stamp, 'July 1, 2015') %></em></span>
    <h3 class="text-muted no-top-margin"><%= @physical.title %></h3>
    <p><%= @physical.notes %></p>
    <p>
      <strong>Owner:</strong> <%= @physical.owner.owner_name %>
    </p>
    <p>
      <strong>Master status:</strong>
      <%= @physical.master_status %>
    </p>
  </div>
</div>